{
    "europaphp.main.index-web": {
        "pattern": "(?<action>GET) (index)?",
        "controller": "europaphp/main/controller/index"
    },
    "europaphp.main.index-cli": {
        "pattern": "(index)?",
        "controller": "europaphp/main/controller/index",
        "action": "cli"
    },
    "europaphp.main.module-cli": {
        "pattern": "module",
        "controller": "europaphp/main/controller/module",
        "action": "cli"
    },
    "europaphp.main.modules-cli": {
        "pattern": "modules",
        "controller": "europaphp/main/controller/modules",
        "action": "cli"
    }
}